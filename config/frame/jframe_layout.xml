                  <component id="com_demo3"/>
                  <component id="com_demo4"/>
                </vertical>
              </horizontal>
            </view>
            <popup>
              <component load="false" id="com_demo4"/>
            </popup>
          </layout>
        </module>
      </module>
    </module>
  </system>
  <system name="system #2" module="module #1" defaultSeat="seat #2">
    <module name="module #1" submodule="">
      <layout>
        <view>
          <horizontal collapsible="0:false;1:false" scales="1:1">
            <component id="com_demo2"/>
