    <module name="module #1" submodule="">
      <layout>
        <view>
          <horizontal scales="1.1:2.9" collapsibile="0:true;1:true" opaqueResize="false">
            <component id="com_demo1"/>
            <vertical scales="1.2:2.8" collapsibile="0:true;1:true" opaqueResize="false">
              <component id="com_demo2"/>
              <component id="com_demo3"/>
            </vertical>
          </horizontal>
