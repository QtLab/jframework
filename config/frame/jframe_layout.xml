        <popup>
          <component load="false" id="com_demo4"/>
        </popup>
      </layout>
    </module>
    <module name="module #2" submodule="">
      <layout>
        <view>
          <horizontal collapsible="0:false;1:false" scales="1.2:3.8">
            <component id="com_demo2"/>
            <component id="com_demo1"/>
          </horizontal>
        </view>
        <popup>
          <component load="false" id="com_demo4"/>
        </popup>
      </layout>
      <module name="module #3" submodule="">
