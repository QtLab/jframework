<?xml version="1.0" encoding="utf-8" ?>
<jframe version="1.0.0">
  <envval>
    <global>
      <path base="jframe" type="prefix">
        <item path="3rdpart" />
        <item path="kernel" />
        <item path="kernel/bcg"/>
        <item path="kernel/qt" />
      </path>
      <path base="app" type="append">
      </path>
    </global>
  </envval>
  <qtconf desc="qt.conf content">
    <paths>
      <item key="Plugins" value="$(jframe)" />
      <item key="Imports" value="$(jframe)" />
      <item key="Qml2Import" value="$(jframe)" />
    </paths>
  </qtconf>
  <database>
    <serverName>****</serverName>
    <userName>****</userName>
    <password>****</password>
  </database>
  <workMode id="1" name="Test1" dir="WorkMode_Test1" >
  </workMode>
  <system>
    <language name="zh_CN" path="$(jframe)">
    </language>
  </system>
  <mainWindow
    windowTitle="框架测试程序"
    windowIcon="@ConfigDir@/resource/app.ico"
    windowTheme="Office2010Blue"
    imageSplash="@ConfigDir@/resource/splash.png"
    imageFinish="@ConfigDir@/resource/finish.png"
    closeKilled="true"
    systemFont="Microsoft Yahei,9">
      <ribbonBar
        visible="true"
        minimized="false" />
    </mainWindow>
</jframe>
