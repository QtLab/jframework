<?xml version="1.0" encoding="utf-8" ?>
<jframe version="1.0.0">
  <envval>
    <global>
      <path base="@FrameDir@/bin" type="prefix">
        <item path="3rdpart" />
        <item path="3rdpart/BCGCBPro" />
        <item path="3rdpart/Qt" />
        <item path="jframe" />
      </path>
      <path base="@AppDir@" type="append">
      </path>
    </global>
  </envval>
  <qtconf desc="qt.conf content">
    <paths>
      <item key="Plugins" value="@FrameDir@/bin/3rdpart/Qt/plugins" />
      <item key="Imports" value="@FrameDir@/bin/3rdpart/Qt/imports" />
      <item key="Qml2Import" value="@FrameDir@/bin/3rdpart/Qt/qml" />
    </paths>
  </qtconf>
  <database>
    <serverName>****</serverName>
    <userName>****</userName>
    <password>****</password>
  </database>
  <workMode id="1" name="Test1" dir="WorkMode_Test1" >
  </workMode>
  <system>
    <language name="zh_CN" path="@FrameDir@/config/language">
    </language>
  </system>
  <textCodec encoding="GBK" />
  <logging filePath="@ConfigDir@/frame/jframe_logging.conf" ></logging>
  <mainWindow
    windowTitle="框架测试程序"
    windowIcon="@ConfigDir@/resource/app.ico"
    windowTheme="Office2010Blue"
    closeKilled="true"
    systemFont="Microsoft Yahei, 9">
    toolBarType="ribbon"
    layoutType="splitter"
    <splash>
      <imageStart>@ConfigDir@/resource/splash.png</imageStart>
      <imageFinish>@ConfigDir@/resource/finish.png</imageFinish>
      <font>Microsoft Yahei, 24, blue</font>
      <msgAlign>AlignCenter</msgAlign>
    </splash>
    <ribbonBar
      visible="true"
      minimized="false" />
  </mainWindow>
</jframe>
